

int c = 0;

void setup() {
  // put your setup code here, to run once:
  Serial.begin(9600);
  //pinMode(2, INPUT);
  pinMode(4, OUTPUT);
  pinMode(7, OUTPUT);
}


void loop() {
  // put your main code here, to run repeatedly:
  while(c < 2){
    digitalWrite(4, HIGH);
    //digitalWrite(7, HIGH);
    analogWrite(5, 255);
    //analogWrite(6, 255);
    delay(1000);
    c++;
  }
